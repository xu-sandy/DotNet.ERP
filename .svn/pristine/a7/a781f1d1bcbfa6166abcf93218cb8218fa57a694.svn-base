/*Home-Index*/
 .topnav { min-width: 200px; width: auto; padding: 40px 28px 25px 0; font-family: "CenturyGothicRegular", "Century Gothic", Arial, Helvetica, sans-serif;  }

    ul.topnav { padding: 0; margin: 0; font-size: 1em; line-height: 1em; list-style: none; }

    ul.topnav li { }

    ul.topnav li a { font-size: 11px; padding: 15px 5px; color: #000; display: block; text-decoration: none; /*font-weight: bolder;*/ border-bottom: 1px solid rgb(210,215,216); cursor: pointer; }

    ul.topnav li a:hover { /*color: white;*/ background-color: #f1f8fe; }

    ul.topnav li a.tree { background: #f5f5f5; background: -moz-linear-gradient(top,#f5f5f5,#f1f3f2); background: -webkit-gradient(linear, 0% 0%, 0% 100%,from(#f5f5f5), to(#f1f3f2)); FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#f5f5f5,endColorStr=#f1f3f2); }

    ul.topnav li a.tree:hover { background-color: #f1f8fe; background: -moz-linear-gradient(top,#e8eff5,#f1f8fe); background: -webkit-gradient(linear, 0% 0%, 0% 100%,from(#e8eff5), to(#f1f8fe)); FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#e8eff5,endColorStr=#f1f8fe); }
    ul.topnav li a.select { background-color: #ffe48d !important; background: -moz-linear-gradient(top,#ffe48d,#ffe48d) !important; background: -webkit-gradient(linear, 0% 0%, 0% 100%,from(#ffe48d), to(#ffe48d)) !important; FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#ffe48d,endColorStr=#ffe48d) !important; }
    ul.topnav ul { margin: 0; padding: 0; display: none; }

    ul.topnav .closedSign { padding-left: 10px; padding-right: 10px; background: url("~/Content/mythemes/default/images/home/closedSign.png") no-repeat center; }

    ul.topnav .openedSign { padding-left: 10px; padding-right: 10px; background: url("~/Content/mythemes/default/images/home/openedSign.png") no-repeat center;  }
    ul.topnav .color { width: 8px; height: 8px; margin: 0; margin-right: 20px; float: right; font-size: 1px; -moz-border-radius: 3px 3px 3px 3px; -webkit-border-radius: 3px 3px 3px 3px; border-radius: 3px 3px 3px 3px; background-color: rgb(16,208,231); background: no-repeat center;*margin-top:-25px;_margin-top:-25px;}
    ul.topnav .color1 { background-color: rgb(16,208,231); }

    ul.topnav .color2 { background-color: rgb(152,208,35); }

    ul.topnav .color3 { background-color: rgb(242,186,15); }

    ul.topnav .color4 { background-color: rgb(241,50,131); }
